
@{
    ViewBag.Title = "Unauthorized access";
}

<div class="error">
    <div class="error__content">
        <h2>Unauthorized</h2>
        <h3>You've unauthorized access to this page!</h3>

        <p>@TempData["ErrorMessage"]</p>
        <button type="button" class="btn btn-primary btn-pill" onclick="previouspage()">&larr; Go Back</button>
    </div> <!-- / .error_content -->
</div> <!-- / .error -->

@section Scripts{
    <script>
        function previouspage(){
            history.back();
        }
    </script>
}
