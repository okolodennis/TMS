<div class="container-fluid px-0" style="display:none" id="alert">
    <div class="alert @TempData["AlertType"] alert-dismissible fade show m-0" role="alert">
        @TempData["AlertMessage"] <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
</div>